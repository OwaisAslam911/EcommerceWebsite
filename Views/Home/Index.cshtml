@{
    ViewData["Title"] = "Home Page";
}
<div class="main-content mx-4 mt-1">
    <div class="conte ">
        <div class="row">
            <div class="Categories-table d-flex justify-content-between m-2 text-center">

                <div class="single-category ">
                    <a class="category-link " href="#">
                        <div class="category-img">
                            <img class="category-image" src="assets/category/grocery.webp" alt="">
                        </div>
                        <span class="category-span ">Grocery</span>
                    </a>
                </div>

                <div class="single-category">
                    <a class="category-link" href="#">
                        <div class="category-img">
                            <img class="category-image" src="assets/category/electronics.png" alt="">
                        </div>
                        <span class="category-span">Electronics</span>
                    </a>
                </div>

                <div class="single-category">
                    <a class="category-link" href="#">
                        <div class="category-img">
                            <img class="category-image" src="assets/category/mobile.webp" alt="">
                        </div>
                        <span class="category-span">Mobiles</span>
                    </a>
                </div>

                <div class="single-category">
                    <a class="category-link" href="#">
                        <div class="category-img">
                            <img class="category-image" src="assets/category/fashion.webp" alt="">
                        </div>
                        <span class="category-span">Fashion</span>
                    </a>
                </div>

                <div class="single-category">
                    <a class="category-link" href="#">
                        <div class="category-img">
                            <img class="category-image" src="assets/category/furniture.webp" alt="">
                        </div>
                        <span class="category-span">Home/Furniture</span>
                    </a>
                </div>

                <div class="single-category">
                    <a class="category-link" href="#">
                        <div class="category-img">
                            <img class="category-image" src="assets/category/appliances.webp" alt="">
                        </div>
                        <span class="category-span">Appliances</span>
                    </a>
                </div>

                <div class="single-category">
                    <a class="category-link" href="#">
                        <div class="category-img">
                            <img class="category-image" src="assets/car1.jfif" alt="">
                        </div>
                        <span class="category-span">Grocery</span>
                    </a>
                </div>

                <div class="single-category">
                    <a class="category-link" href="#">
                        <div class="category-img">
                            <img class="category-image" src="assets/category/beauty.png" alt="">
                        </div>
                        <span class="category-span">Beauty</span>
                    </a>
                </div>

                <div class="single-category">
                    <a class="category-link" href="#">
                        <div class="category-img">
                            <img class="category-image" src="assets/category/beauty.png" alt="">
                        </div>
                        <span class="category-span">Toys</span>
                    </a>
                </div>

            </div>
        </div>
    </div>

    <div id="carouselExampleControlsNoTouching" class="carousel slide" data-bs-touch="false">
        <div class="carousel-inner">
            <div class="carousel-item active">
                <img src="assets/car1.jfif" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="assets/car2.jfif" class="d-block w-100" alt="...">
            </div>
            <div class="carousel-item">
                <img src="assets/car3.jfif" class="d-block w-100" alt="...">
            </div>
        </div>
        <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleControlsNoTouching"
                data-bs-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Previous</span>
        </button>
        <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleControlsNoTouching"
                data-bs-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="visually-hidden">Next</span>
        </button>
    </div>
</div>